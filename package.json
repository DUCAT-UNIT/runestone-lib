{
  "name": "@ducat-unit/runestone",
  "version": "2.0.4",
  "description": "Fork of the Magic Eden runestone library.",
  "repository": {
    "type": "git",
    "url": "https://github.com/DUCAT-UNIT/runestone-lib"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "restricted"
  },
  "unpkg": "./dist/script.js",
  "files": [
    "README.md",
    "CHANGELOG.md",
    "LICENSE",
    "dist",
    "src"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "script": "tsx --tsconfig ./test/tsconfig.json",
    "test": "npm run script test/tape.ts",
    "build": "./scripts/build.sh",
    "check": "npx tsc --project tsconfig.json --noEmit",
    "lint": "biome check src/",
    "lint:fix": "biome check src/ --apply",
    "package": "./scripts/package.sh",
    "release": "./scripts/release.sh",
    "start": "node index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "2.4.2",
    "@types/node": "25.3.0",
    "@types/tape": "5.8.1",
    "faucet": "0.0.4",
    "tape": "5.9.0",
    "tslib": "2.8.1",
    "tsx": "4.21.0",
    "typescript": "5.9.3"
  },
  "dependencies": {
    "@vbyte/buff": "^1.1.0"
  }
}
