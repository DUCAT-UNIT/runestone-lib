{
  "name": "@ducat-unit/runestone",
  "version": "2.0.1",
  "description": "Fork of the Magic Eden runestone library.",
  "repository": {
    "type": "git",
    "url": "https://github.com/DUCAT-UNIT/runestone-lib"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "restricted"
  },
  "unpkg": "./dist/script.js",
  "files": [
    "README.md",
    "CHANGELOG.md",
    "LICENSE",
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "script": "tsx",
    "test": "npm run script test/tape.ts",
    "build": "./scripts/build.sh",
    "check": "npx tsc --project tsconfig.json --noEmit",
    "lint": "biome check src/lib src/types test index.ts",
    "lint:fix": "biome check src/lib src/types test index.ts --apply",
    "package": "./scripts/package.sh",
    "release": "./scripts/release.sh",
    "start": "node index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "2.3.13",
    "@types/node": "^20.10.7",
    "@types/tape": "^5.8.1",
    "faucet": "^0.0.4",
    "tape": "^5.9.0",
    "tsx": "^4.21.0",
    "typescript": "^5.3.3"
  }
}
